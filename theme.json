{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
	
		"layout": {
			"contentSize": "1140px",
			"wideSize": "1400px"
		},

		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "10px",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "20px",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "30px",
					"slug": "40"
				},
				{
					"name": "Regular",
					"size": "clamp(30px, 5vw, 50px)",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "clamp(30px, 7vw, 70px)",
					"slug": "60"
				},
				{
					"name": "X-Large",
					"size": "clamp(50px, 7vw, 90px)",
					"slug": "70"
				},
				{
					"name": "XX-Large",
					"size": "clamp(70px, 10vw, 140px)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"writingMode": true,
			"defaultFontSizes": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "16px",
					"slug": "medium"
				},
				{

					"name": "Large",
					"size": "20px",
					"slug": "large"
				},
				{

					"name": "Extra Large",
					"size": "22px",
					"slug": "x-large"
				},
				{
		
					"name": "Extra Extra Large",
					"size": "45",
					"slug": "xx-large"
				},
				{
		
					"name": "Gigantic",
					"size": "60",
					"slug": "xxx-large"
				}
			],
			"fontFamilies": [
				{
					"slug": "bricolage-grotesque",
					"name": "Bricolage Grotesque",
					"fontFamily": "'Bricolage Grotesque', sans-serif"
				},
				{
					"slug": "gothic-a1",
					"name": "Gothic A1",
					"fontFamily": "'Gothic A1', sans-serif"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "#8741D3",
					"text": "#ffffff"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gothic-a1)",
					"fontWeight": "700"
				}
			}
		},
		"elements": {
			"h2": {
				"typography": {
				"fontFamily": "var(--wp--preset--font-family--bricolage-grotesque)"
				}
			}
		}
	}

}
